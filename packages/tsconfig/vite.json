{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "./base.json",
	"Display": "Vite",
	"compilerOptions": {
		"target": "ESNext",
		"lib": ["DOM", "DOM.Iterable", "ESNext"],
		"types": ["vite/client"],
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"noImplicitAny": false,
		"skipLibCheck": false,
		"declaration":true,
		"esModuleInterop": false,
		"module": "ESNext",
		"preserveSymlinks": false,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true,
		"jsx": "react-jsx"
	}
}
